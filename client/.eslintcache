bmit} className=\"login-form\">\n            <Header as='h2'>Login</Header>\n            <List className=\"errors\">\n                {Object.values(errors).map(err => <List.Item>{err}</List.Item>)}\n            </List>\n            <Form.Field>\n                <Form.Input \n                    label=\"Username\"\n                    name=\"username\"\n                    placeholder=\"Username\"\n                    onChange={handleInputChange}\n                    value={formState.username}\n                />\n            </Form.Field>\n            <Form.Field>\n                <Form.Input\n                    label=\"Password\"\n                    type=\"password\"\n                    name=\"password\"\n                    placeholder=\"Password\"\n                    onChange={handleInputChange}\n                    value={formState.password}\n                />\n            </Form.Field>\n            <Button primary type=\"submit\">Submit</Button>\n        </Form>\n    )\n};\n\nconst LOGIN_USER = gql`\n    mutation login(\n        $username: String!\n        $password: String!\n    ) {\n        login(loginInput: {\n            username: $username\n            password: $password\n        }) {\n            user {\n                username\n                email\n            }\n            token\n        }\n    }\n`;\n\n\nexport default LoginForm;\n","/home/brandon/Documents/projects/fp-rankings2/client/src/components/pages/Login-Register/index.js",[],"/home/brandon/Documents/projects/fp-rankings2/client/src/context/auth.js",[],"/home/brandon/Documents/projects/fp-rankings2/client/src/components/pages/Home/index.js",[],"/home/brandon/Documents/projects/fp-rankings2/client/src/components/pages/Rankings/index.js",[],"/home/brandon/Documents/projects/fp-rankings2/client/src/components/pages/User/index.js",[],"/home/brandon/Documents/projects/fp-rankings2/client/src/components/routes/ProtectedRoute.js",[],"/home/brandon/Documents/projects/fp-rankings2/client/src/